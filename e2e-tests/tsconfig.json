// https://docs.cypress.io/app/tooling/typescript-support?utm_source=chatgpt.com#Configure-tsconfigjson
{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "sourceMap": true,
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@config/*": ["cypress/support/config/*"],
      "@typings/*": ["cypress/support/types/*"]
    }
  },
  "include": ["**/*.ts"]
}